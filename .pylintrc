[MASTER]
init-hook='import sys; sys.path.append("/root/wrm/wrm_api/")'
load-plugins=pylint_django
django-settings-module=wrm_api.settings

[MESSAGES CONTROL]
disable=
    missing-docstring,
    missing-class-docstring,
    missing-module-docstring,
    no-member,
    unused-argument,

[FORMAT]
max-line-length=120